{
  "name": "yumyumapp",
  "version": "1.0.0",
  "description": "Full-stack web application with Django backend and React frontend",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd . && python manage.py runserver",
    "migrate": "python manage.py migrate && npm run backend",
    "backend_unit_tests": "python manage.py test api.views_test",
    "frontend": "cd frontend && npm run dev",
    "install-all": "npm install && cd frontend && npm install",
    "build": "cd frontend && npm run build",
    "lint": "cd frontend && npm run lint"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "django",
    "react",
    "fullstack",
    "yumyumapp"
  ],
  "author": "",
  "license": "MIT"
}
